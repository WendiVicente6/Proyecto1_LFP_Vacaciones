digraph Gramatica {
	layout=dot rankdir=LR shape=circle
	A [shape=doublecircle]
	B [shape=circle]
	C [shape=circle]
	Inicio [shape=plaintext]
	Inicio -> A
	A -> A [label=1]
	A -> B [label=0]
	B -> B [label=1]
	C -> C [label=1]
	C -> C [label=0]
	B -> C [label=0]
	"('AFD_2', ['A', 'B', 'C'], ['0', '1'], 'A', ['A'], [('A', '1', 'A'), ('A', '0', 'B'), ('B', '1', 'B'), ('C', '1', 'C'), ('C', '0', 'C'), ('B', '0', 'C')])" [shape=box]
}
