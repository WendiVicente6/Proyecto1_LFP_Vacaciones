digraph Gramatica {
	layout=dot rankdir=LR shape=circle
	A [shape=circle]
	B [shape=circle]
	C [shape=doublecircle]
	D [shape=circle]
	Inicio [shape=plaintext]
	Inicio -> A
	A -> B [label=0]
	A -> C [label=1]
	B -> A [label=0]
	B -> D [label=1]
	C -> D [label=0]
	C -> A [label=1]
	D -> C [label=0]
	D -> A [label=1]
	"Nombre: AFD_2
No terminales: A,B,C,D
Terminales: 0,1
No terminal Inicial: A
Producciones:
A>0B
| 1C
B>0A
| 1D
C>0D
| 1A
| $
D>0C
| 1A" [shape=box]
}
