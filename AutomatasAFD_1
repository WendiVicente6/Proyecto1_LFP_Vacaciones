digraph Gramatica {
	layout=dot rankdir=LR shape=circle
	A [shape=circle]
	B [shape=circle]
	C [shape=doublecircle]
	D [shape=circle]
	Inicio [shape=plaintext]
	Inicio -> A
	A -> B [label=1]
	A -> C [label=0]
	B -> A [label=1]
	B -> D [label=0]
	C -> D [label=1]
	C -> A [label=0]
	D -> C [label=1]
	D -> B [label=0]
	"('AFD_1', ['A', 'B', 'C', 'D'], ['0', '1'], 'A', ['C'], [('A', '1', 'B'), ('A', '0', 'C'), ('B', '1', 'A'), ('B', '0', 'D'), ('C', '1', 'D'), ('C', '0', 'A'), ('D', '1', 'C'), ('D', '0', 'B')])" [shape=box]
}
